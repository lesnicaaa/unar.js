"column":70},"source":"..\\src\\View\\Detective\\index.js","original":{"line":48,"column":31}},{"name":"cb","generated":{"line":113,"column":72},"source":"..\\src\\View\\Detective\\index.js","original":{"line":48,"column":44}},{"generated":{"line":113,"column":74},"source":"..\\src\\View\\Detective\\index.js","original":{"line":48,"column":31}},{"generated":{"line":113,"column":75},"source":"..\\src\\View\\Detective\\index.js","original":{"line":48,"column":16}},{"generated":{"line":113,"column":76},"source":"..\\src\\View\\Detective\\index.js","original":{"line":48,"column":8}},{"generated":{"line":114,"column":0},"source":"..\\src\\View\\Detective\\index.js","original":{"line":49,"column":5}},{"name":"Detictive","generated":{"line":120,"column":18},"source":"..\\src\\View\\Detective\\index.js","original":{"line":10,"column":21}}],"sources":{"..\\src\\View\\Detective\\index.js":"/**\r\n * Detictive\r\n * 静态方法包含this关键字，这个this指的是类\r\n */\r\nimport config from \"../../Config\"\r\nimport DomFn from \"../DomEvent\"\r\nimport Hub from \"../../Hub\"\r\nimport Unar from \"../../../src/Instance\"\r\ndebugger\r\nexport default class Detictive {\r\n    static [config.actionPrefix + \"-model\"](node, key, vm) {\r\n        debugger\r\n        this._update(\"model\", node, key, vm)\r\n    }\r\n    static [config.actionPrefix + \"-text\"](node, text, vm) {\r\n        this._update(\"text\", node, text)\r\n    }\r\n    static [config.actionPrefix + \"-html\"](node, html, vm) {\r\n        this._update(\"html\", node, html)\r\n    }\r\n    //:\r\n    static bind(node, attrName, attrVal) {\r\n        DomFn.bind(node, attrName, attrVal)\r\n    }\r\n    //@\r\n    static addEvt(node, attrName, fn) {\r\n        var evtName = attrName.substr(config.evtPrefix.length, attrName.length)\r\n        var fn = this.methods[fn].bind(this)\r\n        DomFn.addEvt(node, evtName, fn)\r\n    }\r\n    static _update(detictive, node, key, vm) {\r\n        var cb = DomFn[detictive]\r\n        if(key==='model'){\r\n            node.addEventListener('input',e=>{\r\n                vm[key]=e.target.value\r\n            },false)\r\n        }\r\n        cb(node, vm[key])\r\n        //检测hubs 是否具备此prop（value）hub，有的添加cb回调，没有创建便hub\r\n        var has = false\r\n        for (let hub of Unar.hubs) {\r\n            if (hub.prop === key) {\r\n                hub.listeners.push(cb)\r\n                has = true\r\n                break\r\n            }\r\n        }\r\n        !has && Unar.hubs.push(new Hub(key, cb))\r\n    }\r\n}"},"lineCount":null}},"hash":"df7166ab5ffa9c94a32e3edbc0d4fe39","cacheData":{"env":{}}}